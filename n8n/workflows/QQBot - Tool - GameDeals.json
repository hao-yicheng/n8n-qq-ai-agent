    {
      "parameters": {
        "mode": "runOnceForEachItem",
        "jsCode": "// 假设输入是单个游戏的 JSON 对象 ($input.item.json)\nconst game = $input.item.json;\n\n// 获取游戏名（从前一个 Split Out 节点）\nconst title = $('SplitGameList').item.json.games; // 假设 title 字段包含游戏名\n\n// 如果没有 deals，直接返回一个标记对象\nif (!game.deals || game.deals.length === 0) {\n  $input.item.json = {\n    title,\n    error: \"No deals found\"\n  };\n  return $input.item;\n}\n\n// 取第一个 deal（可改为按价格排序）\nconst bestDeal = game.deals[0];\n\n// 安全获取 currency 和历史最低价\nconst currency = game?.historyLow?.all?.currency \n              || bestDeal?.price?.currency \n              || \"N/A\";\nconst histLowAll = game?.historyLow?.all?.amount ?? null;\n\nconst currentPrice = bestDeal?.price?.amount ?? null;\nconst originalPrice = bestDeal?.regular?.amount ?? null;\nconst discount = bestDeal?.cut ?? 0;\nconst onSale = discount > 0;\nconst isHistLow = histLowAll !== null && currentPrice !== null && currentPrice === histLowAll;\n\n// 格式化价格为字符串\nconst formatPrice = (amount, currency) => {\n  if (amount === null || amount === undefined) return \"N/A\";\n  return `${amount.toFixed(2)} ${currency}`;\n};\n\n// 写入简化 JSON\n$input.item.json = {\n  title,                         \n  onSale,                       \n  isHistLow,                    \n  currentPrice: formatPrice(currentPrice, currency),  \n  discount: onSale ? discount : 0,                   \n  originalPrice: formatPrice(originalPrice, currency), \n  histLow: histLowAll !== null ? formatPrice(histLowAll, currency) : \"N/A\"\n};\n\n// 返回处理后的项\nreturn $input.item;"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
